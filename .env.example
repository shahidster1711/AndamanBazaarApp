# Supabase Configuration (REQUIRED)
# Get these from: https://supabase.com/dashboard/project/<project-id>/settings/api
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# Google Gemini AI API Key (REQUIRED for image moderation)
# Get from: https://makersuite.google.com/app/apikey
VITE_API_KEY=your-gemini-api-key-here

# Firebase Hosting Configuration (used only for deployment via firebase deploy)
# Get these from: https://console.firebase.google.com/project/your-project/settings/general
VITE_FIREBASE_API_KEY=your-firebase-api-key-here
VITE_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project-id.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your-messaging-sender-id
VITE_FIREBASE_APP_ID=1:your-app-id:web:your-app-id
VITE_FIREBASE_MEASUREMENT_ID=G-YOUR-MEASUREMENT-ID

# Security Configuration (optional - defaults provided)
VITE_RATE_LIMIT_WINDOW=60
VITE_RATE_LIMIT_MAX_REQUESTS=20
VITE_ENABLE_CSP=true

# Environment (optional)
VITE_ENV=development
