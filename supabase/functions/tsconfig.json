{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "node",
        "lib": [
            "esnext",
            "dom",
            "dom.iterable"
        ],
        "strict": true,
        "isolatedModules": true,
        "noEmit": true,
        "forceConsistentCasingInFileNames": true,
        "types": [
            "deno"
        ],
        "baseUrl": ".",
        "paths": {
            "https://esm.sh/@supabase/supabase-js@2": [
                "../../node_modules/@supabase/supabase-js"
            ],
            "npm:cashfree-pg@^5.1.0": [
                "../../node_modules/cashfree-pg"
            ]
        }
    },
    "include": [
        "**/*.ts"
    ]
}